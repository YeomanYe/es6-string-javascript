{
  "name": "es6-string-shell",
  "displayName": "es6-string-shell",
  "description": "Highlight shell language in ES6 template literals",
  "version": "1.0.1",
  "publisher": "zjcompt",
  "icon": "docs/logo.png",
  "engines": {
    "vscode": "^1.19.0"
  },
  "galleryBanner": {
    "color": "#dedede",
    "theme": "light"
  },
  "repository": {
    "url": "https://github.com/zjcompt/es6-string-javascript"
  },
  "bugs": {
    "url": "https://github.com/zjcompt/es6-string-javascript/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "javascript",
    "es6",
    "lit-javascript",
    "lit-html"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx",
          "source.sh"
        ],
        "scopeName": "inline.es6-shell",
        "path": "./syntaxes/es6-inline-shell.json",
        "embeddedLanguages": {
          "meta.embedded.block.shellscript": "shellscript"
        }
      }
    ]
  }
}
